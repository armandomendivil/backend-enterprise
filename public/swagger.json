{
 "swagger": "2.0",
 "info": {
  "version": "1.0.0",
  "title": "Backend API"
 },
 "host": "localhost:9000",
 "basePath": "/",
 "schemes": [
  "http"
 ],
 "consumes": [
  "application/json"
 ],
 "produces": [
  "application/json"
 ],
 "paths": {
  "/api/user/register": {
   "post": {
    "summary": "Register an user",
    "description": "Register an user with role",
    "operationId": "register",
    "parameters": [
     {
      "name": "email",
      "in": "formData",
      "description": "The user email",
      "required": true,
      "type": "string"
     },
     {
      "name": "password",
      "in": "formData",
      "description": "The user password",
      "required": true,
      "type": "string"
     }
    ],
    "responses": {
     "200": {
      "description": "User registration went successful"
     },
     "404": {
      "description": "Route not found"
     },
     "500": {
      "description": "Something went wrong"
     }
    }
   }
  },
  "/api/user/login": {
   "post": {
    "summary": "Login Authentication",
    "description": "Login authentication with email and password",
    "operationId": "login",
    "parameters": [
     {
      "name": "email",
      "in": "formData",
      "description": "The user email",
      "required": true,
      "type": "string"
     },
     {
      "name": "password",
      "in": "formData",
      "description": "The user password",
      "required": true,
      "type": "string"
     }
    ],
    "responses": {
     "200": {
      "description": "User login went successful"
     },
     "404": {
      "description": "Route not found"
     },
     "500": {
      "description": "Something went wrong"
     }
    }
   }
  }
 }
}